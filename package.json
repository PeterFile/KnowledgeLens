{
  "name": "knowledgelens",
  "version": "0.1.3",
  "type": "module",
  "description": "AI-powered browser assistant for reading and knowledge management",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest --run",
    "test:watch": "vitest",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,css}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,css}\"",
    "prepare": "husky",
    "release": "standard-version",
    "release:first": "standard-version --first-release",
    "release:prep": "standard-version --skip.tag",
    "release:tag": "node scripts/tag-release.cjs"
  },
  "keywords": [
    "chrome-extension",
    "ai",
    "knowledge-management"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@huggingface/transformers": "^3.4.1",
    "@orama/orama": "^3.1.5",
    "@orama/plugin-data-persistence": "^3.1.5",
    "@tailwindcss/typography": "^0.5.19",
    "idb": "^8.0.2",
    "js-tiktoken": "^1.0.21",
    "onnxruntime-web": "^1.23.2",
    "qrcode": "^1.5.4",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-markdown": "^10.1.0",
    "remark-gfm": "^4.0.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.2.0",
    "@commitlint/config-conventional": "^20.2.0",
    "@crxjs/vite-plugin": "^2.0.0-beta.32",
    "@eslint/js": "^9.39.1",
    "@rollup/rollup-linux-x64-gnu": "^4.54.0",
    "@types/chrome": "^0.0.287",
    "@types/pngjs": "^6.0.5",
    "@types/qrcode": "^1.5.6",
    "@types/react": "^19.1.4",
    "@types/react-dom": "^19.1.5",
    "@vitejs/plugin-react": "^4.4.1",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "fast-check": "^4.4.0",
    "happy-dom": "^20.0.11",
    "husky": "^9.1.7",
    "jsqr": "^1.4.0",
    "lint-staged": "^16.2.7",
    "pngjs": "^7.0.0",
    "postcss": "^8.5.3",
    "prettier": "^3.7.4",
    "standard-version": "^9.5.0",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.49.0",
    "vite": "^6.3.5",
    "vite-plugin-static-copy": "^3.1.4",
    "vitest": "^4.0.15"
  }
}
